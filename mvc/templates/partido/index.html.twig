{% extends 'base.html.twig' %}

{% block title %}Partido index{% endblock %}

{% block body %}
    <h1>Partido index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for partido in partidos %}
            <tr>
                <td>{{ partido.id }}</td>
                <td>
                    <a href="{{ path('app_partido_show', {'id': partido.id}) }}">show</a>
                    <a href="{{ path('app_partido_edit', {'id': partido.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_partido_new') }}">Create new</a>
{% endblock %}
