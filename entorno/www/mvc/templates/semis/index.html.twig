{% extends 'base.html.twig' %}

{% block title %}Semis index{% endblock %}

{% block body %}
    <h1>Semis index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for semi in semis %}
            <tr>
                <td>{{ semi.id }}</td>
                <td>
                    <a href="{{ path('app_semis_show', {'id': semi.id}) }}">show</a>
                    <a href="{{ path('app_semis_edit', {'id': semi.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_semis_new') }}">Create new</a>
{% endblock %}
